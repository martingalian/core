{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "martingalian/core",
    "type": "project",
    "description": "Martingalian Core",
    "keywords": [
        "crypto",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "angle/chrono": "^1.0",
        "binance/binance-connector-php": "^1.6",
        "brunocfalcao/laravel-helpers": "dev-master",
        "laravel-notification-channels/pushover": "^4.0",
        "laravel/framework": "^12.0",
        "predis/predis": "^2.2",
        "ratchet/pawl": "^0.4.1",
        "spatie/laravel-model-states": "2.11.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/nova-devtool": "^1.8",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "nunomaduro/phpinsights": "^2.13",
        "phploc/phploc": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Martingalian\\Core\\": "src/",
            "Martingalian\\Core\\Database\\Factories\\": "database/factories/",
            "Martingalian\\Core\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Martingalian\\Core\\CoreServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
